<?xml version="1.0"?>
<!DOCTYPE config>

<config>
    <modules>
        <SoftUni_Tulev>
            <version>0.0.0</version>
        </SoftUni_Tulev>
    </modules>
    <global>
        <models>
            <softuni_tulev>
                <class>SoftUni_Tulev_Model</class>
                <resourceModel>softuni_tulev_resource</resourceModel>
            </softuni_tulev>
            <softuni_tulev_resource>
                <class>SoftUni_Tulev_Model_Resource</class>
                <entities>
                    <submission>
                        <table>softuni_tulev_submission</table>
                    </submission>
                </entities>
            </softuni_tulev_resource>
        </models>
        <blocks>
            <softuni_tulev>
                <class>SoftUni_Tulev_Block</class>
            </softuni_tulev>
        </blocks>
        <helpers>
            <softuni_tulev>
                <class>SoftUni_Tulev_Helper</class>
            </softuni_tulev>
        </helpers>
        <resources>
            <softuni_tulev_setup>
                <setup>
                    <module>SoftUni_Tulev</module>
                </setup>
            </softuni_tulev_setup>
        </resources>
    </global>
    <frontend>
        <routers>
            <submission>
                <use>standard</use>
                <args>
                    <frontName>submission</frontName>
                    <module>SoftUni_Tulev</module>
                </args>
            </submission>
        </routers>
    </frontend>
</config>